(res.data.user.user)\n                setUser(JSON.stringify(res.data.user.user))\n                // setUser('success')\n\n                history.push(\"/profile\");\n              })\n              .catch((err) => {\n                console.log(err);\n              });\n        }\n    };\n\n    return (\n        <div>\n            {/* <Navbar /> */}\n            <div className={classes.container}>\n                <div className={classes.child}>\n                    <Typography className={classes.title} variant='h2'>\n                        Log In\n                    </Typography>\n                    <br />\n                    <form>\n                        <TextField\n                        className={classes.field}\n                        margin=\"normal\"\n                        required\n                        id=\"email\"\n                        onChange={handleInputChange}\n                        name=\"email\"\n                        label=\"Email\"\n                        />\n                        <br/>\n                        <TextField\n                        className={classes.field}\n                        margin=\"normal\"\n                        required\n                        id=\"password\"\n                        onChange={handleInputChange}\n                        name=\"password\"\n                        type='password'\n                        label=\"Password\"\n                        />\n                        <br/>\n                        <Button onClick={handleSubmit} className={classes.button} variant=\"contained\">Login</Button>\n                    </form>\n                    <Typography className={classes.sidenote} variant='p'>\n                        Create an account?\n                        <br/>\n                        <a href='/'>Sign up</a>\n                    </Typography>\n                </div>\n            </div>\n        </div>\n    )\n}\n","/Users/kdoh/food-delivery/client/src/Auth.js",[],"/U